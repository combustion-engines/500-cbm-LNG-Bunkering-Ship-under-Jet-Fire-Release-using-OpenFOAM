FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];

internalField   uniform 0.3379;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    gas
    {
        type            fixedValue;
        value           uniform 2e-4;
    }

    open
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    wall
    {
        type            zeroGradient;
    }

    wall2
    {
        type            zeroGradient;
    }



}


